{
  "name": "scaffold",
  "version": "1.0.0",
  "description": "基于ant.design, dvaJS, Typescript 的 react脚手架",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "install": "npm install",
    "dev": "cross-env env.NODE_ENV=development env.ENV_API_CONTEXT=api  env.ENV_PUBLIC_PATH=/ ESLINT=true PORT=8888 roadhog dev",
    "build:dev": "cross-env env.NODE_ENV=production ESLINT=none env.ENV_API_CONTEXT=api  env.ENV_PUBLIC_PATH=/  roadhog build",
    "build:prd": "cross-env env.NODE_ENV=production env.ENV_API_CONTEXT=api env.ENV_PUBLIC_PATH=/prd ESLINT=none roadhog build",
    "build:stg": "cross-env env.NODE_ENV=production env.ENV_API_CONTEXT=api env.ENV_PUBLIC_PATH=/stg ESLINT=none roadhog build",
    "build:stg2": "cross-env env.NODE_ENV=production env.ENV_API_CONTEXT=api2 env.ENV_PUBLIC_PATH=/stg2 ESLINT=none roadhog build",
    "build:no-uglify": "cross-env env.NODE_ENV=development ESLINT=none roadhog build",
    "lint": "./node_modules/.bin/eslint ./src/",
    "type-check": "tsc --watch",
    "test": "mocha"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@antv/data-set": "0.8.9",
    "@antv/g6": "3.0.7-beta.1",
    "@babel/polyfill": "7.2.5",
    "antd": "3.23.4",
    "bizcharts": "3.4.3",
    "bizcharts-plugin-slider": "2.1.1",
    "braft-editor": "2.3.8",
    "classnames": "2.2.6",
    "dva": "2.4.1",
    "dva-loading": "2.0.6",
    "gg-editor": "^3.0.0-beta.1",
    "history": "4.7.2",
    "jsencrypt": "2.3.1",
    "lodash": "4.17.11",
    "lodash-decorators": "4.5.0",
    "moment": "2.24.0",
    "nprogress": "0.2.0",
    "numeral": "2.0.6",
    "omit.js": "1.0.0",
    "path-to-regexp": "2.4.0",
    "prop-types": "15.6.2",
    "rc-drawer-menu": "0.5.7",
    "react": "16.2.0",
    "react-amap": "1.2.8",
    "react-container-query": "0.9.1",
    "react-contexify": "4.1.1",
    "react-copy-to-clipboard": "5.0.1",
    "react-dnd": "9.4.0",
    "react-dnd-html5-backend": "9.4.0",
    "react-document-title": "2.0.3",
    "react-dom": "16.2.0",
    "react-fittext": "1.0.0",
    "react-highlight-words": "0.16.0",
    "react-json-view": "1.19.1",
    "react-markdown": "4.2.2",
    "react-monaco-editor": "0.31.0",
    "react-sparklines": "1.7.0",
    "react-split-pane": "0.1.87",
    "react-syntax-highlighter": "11.0.2",
    "scaffold-core": "0.0.3-alpha.0.4",
    "url-polyfill": "1.1.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-dva-hmr": "0.4.1",
    "babel-plugin-import": "1.11.0",
    "babel-plugin-module-resolver": "3.1.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "cross-env": "5.2.0",
    "cross-port-killer": "1.0.1",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "husky": "3.0.5",
    "mocha": "5.2.0",
    "mockjs": "1.0.1-beta3",
    "prettier": "1.18.2",
    "regenerator-runtime": "0.11.1",
    "roadhog": "2.4.9",
    "roadhog-api-doc": "0.3.4",
    "typescript": "^3.6.2"
  },
  "author": "Objectivezt",
  "license": "MIT"
}